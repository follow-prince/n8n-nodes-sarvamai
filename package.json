{
  "name": "n8n-nodes-sarvamai",
  "version": "0.3.0",
  "description": "n8n community node for Sarvam AI",
  "license": "MIT",
  "homepage": "https://github.com/follow-prince/n8n-nodes-sarvamai",
  "keywords": [
    "n8n-community-node-package",
    "sarvamai",
    "n8n-nodes-sarvamai",
    "n8n-nodes-sarvamai-api"
  ],
  "author": {
    "name": "Elavarasan (Prince)",
    "email": "[EMAIL_ADDRESS]"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/follow-prince/n8n-nodes-sarvamai.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
